      "package": "com.vezhdelit.reactnativetodolist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "743ec509-70ef-4414-ab2e-9fbf0431dcaa"
      }
    }
